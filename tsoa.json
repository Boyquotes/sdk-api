{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*.controller.ts"
  ],
  "spec": {
    "outputDirectory": "docs",
    "specVersion": 3,
    "host": "localhost:3000",
    "basePath": "/api/v1",
    "schemes": [
      "http"
    ],
    "info": {
      "title": "Cede SDK API",
      "version": "1.2.0_sdk2.17.0"
    }
  },
  "routes": {
    "routesDir": "docs",
    "middleware": "express"
  }
}